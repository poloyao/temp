<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IService1" />
                <binding name="Service1Soap" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:5302/Service1.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IService1" contract="DemoWCF.IService1"
                name="BasicHttpBinding_IService1" />
            <endpoint address="http://localhost:4533/Service1.asmx" binding="basicHttpBinding"
                bindingConfiguration="Service1Soap" contract="WebAPI.Service1Soap"
                name="Service1Soap" />
        </client>
    </system.serviceModel>
</configuration>
